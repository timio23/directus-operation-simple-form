{
	"name": "simple-form-operation",
	"description": "Convert JSON to form data and submit",
	"icon": "extension",
	"license": "MIT",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-operation"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.0.4",
		"@directus/utils": "^13.0.3",
		"@types/encodeurl": "^1.0.2",
		"@types/node": "^22.13.14",
		"axios": "^1.8.4",
		"encodeurl": "^2.0.0",
		"typescript": "^5.8.2",
		"vue": "^3.5.13"
	}
}
